{
  "name": "yoomer",
  "version": "1.0.0",
  "description": "This is a clone of Reddit with its browsing features written in React",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "npx eslint --ext .js,.ts,.jsx,.tsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-chemus/yoomer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alex-chemus/yoomer/issues"
  },
  "homepage": "https://alex-chemus.github.io/yoomer",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "body-parser": "^1.20.0",
    "csstype": "^3.0.11",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "open-props": "^1.3.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "snoowrap": "^1.23.0",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.30.1",
    "gh-pages": "^4.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.73.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
